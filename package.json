{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "description": ".",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.js",
  "type": "module",
  "bin": {
    "<%= projectName %>": "bin/index.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "scripts": {
    "lint": "eslint --fix ./src && prettier --write ./src",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha",
    "coverage": "nyc report --reporter=lcov",
    "esm": "rm -rf ./esm && rm -rf types && tsc -p tsconfig.esm.json",
    "cjs": "rm -rf ./cjs && tsc -p tsconfig.cjs.json",
    "build": "npm run esm && npm run cjs",
    "prepublishOnly": "npm run test && npm run build",
    "ci": "npm run lint && npm run build && npm run test && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/<%= projectName %>.git"
  },
  "keywords": [],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/download": "^8.0.1",
    "@types/ejs": "^3.1.0",
    "@types/file-exists": "^5.0.0",
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.9",
    "@types/progress": "^2.0.5",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "@packi_/lint-config": "^0.1.11",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "async": "^3.2.3",
    "command-arguments-helper": "^0.1.1",
    "decompress": "^4.2.1",
    "download": "^8.0.0",
    "ejs": "^3.1.6",
    "eslint-import-resolver-alias": "^1.1.2",
    "execa": "^6.0.0",
    "file-exists": "^5.0.1",
    "file-loader": "^6.2.0",
    "is-dir": "^1.0.0",
    "mkdirp": "^1.0.4",
    "module-alias": "^2.2.2",
    "packagejson": "^1.0.1",
    "@packi_/print": "^0.1.3",
    "@packi_/settings": "^0.1.1",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "tslib": "^2.3.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "_moduleAliases": {
    "~": "lib/"
  }
}
