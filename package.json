{
  "name": "<%= projectNamme %>",
  "version": "0.1.0",
  "description": "A cli project.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "bin": {
    "<%= projectNamme %>": "bin/index.js"
  },
  "types": "lib/",
  "type-check": "tsc",
  "scripts": {
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc report --reporter=lcov",
    "es5": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "prepublishOnly": "rm -rf ./lib && npm run es5",
    "ci": "npm lint && npm run test && npm run es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/<%= projectNamme %>.git"
  },
  "keywords": [],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/download": "^6.2.4",
    "@types/ejs": "^3.0.5",
    "@types/file-exists": "^4.0.0",
    "@types/lodash": "^4.14.165",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/progress": "^2.0.3",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "packi-lint-config": "^0.1.11",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "async": "^2.6.3",
    "command-arguments-helper": "^0.1.1",
    "decompress": "^4.2.1",
    "download": "^8.0.0",
    "ejs": "^3.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "execa": "^5.0.0",
    "file-exists": "^5.0.1",
    "file-loader": "^6.2.0",
    "is-dir": "^1.0.0",
    "mkdirp": "^1.0.4",
    "module-alias": "^2.2.2",
    "packagejson": "^1.0.1",
    "packi-print": "^0.1.3",
    "packi-settings": "^0.1.1",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "tslib": "^2.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "_moduleAliases": {
    "~": "lib/"
  }
}
